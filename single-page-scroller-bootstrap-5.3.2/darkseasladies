    var total_counter = 0
    var random_article_loop = function(){
      total_counter++
      if ( recommended_articles.length < 3 ){
        var random_article = all_articles[Math.floor(Math.random()*all_articles.length)]
        if ( random_article.id != this_article_id ){
          var add_article = false
          if ( recommended_articles.length > 0 ){
            var counter = 1
            recommended_articles.forEach(function(article){
              if ( recommended_ids.indexOf( random_article.id ) < 0 && article.id != this_article_id ){
                add_article = true
              }
            })
          } else {
            add_article = true
          }
          if ( add_article ){
            recommended_articles.push( random_article )
            recommended_ids.push( random_article.id )
          }
        }
        if ( total_counter == all_articles.length * 6 ) return;
        random_article_loop()
      } else {
        if ( recommended_articles.length == 0 ) return;
        var h3 = 'More Articles'
        
        $('.article_recommended').append(
          '<h3>'+h3+'</h3>\
          <ul class="list-unstyled simple_articles clearfix"></ul>'
        )
        console.log('recommended_articles', recommended_articles)
        recommended_articles.forEach(function(a){
          $('.simple_articles').append(
            '<li>\
              <div class="inner">\
                <a class="bg_image" href="'+ a.url + '" style="background: url(' + a.image + ') no-repeat center; background-size: cover;" aria-label="Go to ' + a.title + '">&nbsp;</a>\
                <div class="inner2">\
                  <h2><a href="'+ a.url + '" aria-label="Go to ' + a.title + '">' + a.title +'</a></h2>\
                </div>\
              </div>\
            </li>'
          )
        })
      }
    }
    random_article_loop()
  })
</script>


