  <script>
  function cookie (key, value, options) {
   if (arguments.length > 1 && String(value) !== "[object Object]") {
     options = $.extend({}, options);
     if (value === null || value === undefined) {
         options.expires = -1;
     }
     if (typeof options.expires === "number") {
         var days = options.expires,
             t = options.expires = new Date;
         t.setDate(t.getDate() + days);
     }
     value = String(value);
     return document.cookie = [encodeURIComponent(key), "=", options.raw ? value : encodeURIComponent(value), options.expires ? "; expires=" + options.expires.toUTCString() : "", options.path ? "; path=" + options.path : "; path=/", options.domain ? "; domain=" + options.domain : "", options.secure ? "; secure" : ""].join("");
   }
   options = value || {};
   var decode = options.raw ? function(s) {
       return s;
     } : decodeURIComponent,
     result = (new RegExp("(?:^|; )" + encodeURIComponent(key) + "=([^;]*)")).exec(document.cookie);
   return result && result[1] && result[1] !== "null" ? decode(result[1]) : null;
  }

  var urlRegionObj = {
    mainURL: 'https://darkseas',
    countries: {
      EU: '.com',
      IE: '.co.uk',
      GB: '.co.uk',
      US: '.com'
    }
  }
  function rawGetRequest (url, cb) {
    var start = Date.now()
    var timeout = setTimeout(function(){
      if (start) {
        console.error('request timeout')
        return cb()
      } 
    }, 20000)
    var s = new XMLHttpRequest()
    s.onreadystatechange = function () {
      if (this.readyState === 4) {
        if (this.status === 200) {
          var data = JSON.parse(this.responseText)
          clearTimeout(timeout)
          cb(data)
        } else {
          $(function(){
            $.get('https://api.ipapi.com/api/check?access_key=256b76b033a9f0f4bd9f8e97f912aba9',function(data){
              cb(data)
              console.log(JSON.parse(this.responseText).message)
            })
          })
        }
      }
    }
    s.open('GET', url, true)
    s.send()
  }

  function checkRedirectNeeded (ipRegion, savedRegion, requestedRegion) {
    // console.log('---> ipRegion', ipRegion, 'requestedRegion', requestedRegion, 'savedRegion', savedRegion)
    return !(ipRegion === requestedRegion) || !(savedRegion === requestedRegion)
  }
  
  function initRedirect (ipRegion, region) {
    var returnEarly = 0
    for ( var country in urlRegionObj.countries ){
      if ( region === country ){
        // console.log('region: ' + region + ' ' + country + ': redirecting to ' + country)
        if (location.host.indexOf(urlRegionObj.countries[ country ]) < 0) {
          location.href = urlRegionObj.mainURL + urlRegionObj.countries[ country ]
          returnEarly = 1
        }
        break
      }
    }

    if (returnEarly) return    
  }

  function checkRequestedRegion(url, urlRegionObj){
    for ( var abbrev in urlRegionObj.countries ){
      var regex = new RegExp(urlRegionObj.countries[abbrev]+'(\/|$)','i')
      if ( url.match(regex) ) return abbrev
    }
  }

  function locationCheck(cb){
    var url = window.location.href

    // MANUAL REDIRECT OVERRIDE FOR TESTING
    if ( url.indexOf('redirect=') > -1 ){
      if ( url.indexOf('redirect=false') > -1 || /bot|googlebot|crawler|spider|robot|crawling/i.test(navigator.userAgent) ){
        cookie('location_redirect', 0, {expires:365})
        console.log('REDIRECT OFF')
      }
      if ( url.indexOf('redirect=true') > -1 ){
        cookie('location_redirect', 1, {expires:365})
        console.log('REDIRECT ON')
      }
    }

    var ipRegion = ''
    var savedRegion = cookie('savedRegion')
    // SET REQUESTED REGION TO LOAD APPROPRIATE SCRIPTS, WHILE "region" REPRESENTS PHYSICAL LOCATION
    var requestedRegion = checkRequestedRegion(url, urlRegionObj)
    var redirectCookie = cookie('location_redirect')

    var redirectNeeded = checkRedirectNeeded(ipRegion, savedRegion, requestedRegion)
    if ( redirectNeeded === false ) return

    // IF LOCATION REDIRECT OVERRIDE === 0
    if ( redirectCookie != '0' ){
      var is_bot = /bot|google|baidu|bing|msn|duckduckbot|teoma|slurp|yandex/i.test(navigator.userAgent)
      if ( is_bot ) return cb(requestedRegion)
      if ( ipRegion != '' ){
        console.log("// ipRegion != ''")
        initRedirect(ipRegion, 0, urlRegionObj)
      } else {
        try {
          // $.get('https://api.ipapi.com/api/check?access_key=256b76b033a9f0f4bd9f8e97f912aba9',function(ipapi_data){
          rawGetRequest('https://api.ipapi.com/api/check?access_key=256b76b033a9f0f4bd9f8e97f912aba9', function (ipapi_data) {
            // Set country code cookie for potential needed redirects
            // cookie('savedRegion', ipapi_data.country_code, {expires:365})
            // console.log('ipapi_data:', ipapi_data)
            if (ipapi_data.error) return console.error(ipapi_data)

            ipRegion = ipapi_data.country_code
            if (!ipRegion) return

            var region = ipRegion

            // IS IN EUROPE?
            var europeanCountries = ['CH', 'AL','AD','AT','BY','BE','BA','BG','HR','CY','CZ','DK','EE','FO','FI','FR','DE','GI','GR','HU','IS','IM','IT','RS','LV','LI','LT','LU','MK','MT','MD','MC','ME','NL','NO','PL','PT','RO','RU','SM','RS','SK','SI','ES','SE','UA','VA','RS']
            var inEurope = europeanCountries.indexOf(ipRegion) > -1

            // IS IN SOUTHEAST ASIA?
            var sea_countries = [
              'SG', // SINGAPORE
              'VN', // VIETNAM
              'LA', // LAOS
              'KH', // CAMBODIA
              'TH', // THAILAND
              'TW', // TAIWAN
              'BU', // BURMA?
              'MM', // MYANMAR
              'PH', // PHILIPPINES
              'BN', // BRUNEI DARUSSALAM
              'TL', // TIMOR-LESTE
              'MY', // MALAYSIA
              'BN', // BRUNEI
              'ID' // INDONESIA
            ]
            var in_sea = sea_countries.indexOf(ipRegion) > -1

            //console.log('inEurope: ',inEurope);
            if (inEurope){
              region = 'EU'
              cookie('savedRegion', 'EU', {expires:365})
            }
            if (in_sea){
              region = 'SEA'
              cookie('savedRegion', 'SEA', {expires:365})
            }
            if (ipRegion === 'CA') {
              region = 'US'
              cookie('savedRegion', 'US', {expires:365})
            } 

            redirectNeeded = checkRedirectNeeded(ipRegion, savedRegion, requestedRegion)
            if (!redirectNeeded || window.location !== window.parent.location) return
            initRedirect(ipRegion, region, urlRegionObj)
          })
        } catch (err) {
          console.log('** ERROR location redirect', err)
        }
      }
    } else {
      cb(requestedRegion)
      console.log('** Location Redirect Override **')
    }
  }

  // var location_files_loaded = {
  //   location_eu: false,
  //   location_canada: false
  // }
  // function locationLoader(cb){
  //   //console.log('inside locationLoader')
