<script>
  // ESCAPE HARMFUL CHARS
  var not_safe_in_xml = /[^\x09\x0A\x0D\x20-\xFF\x85\xA0-\uD7FF\uE000-\uFDCF\uFDE0-\uFFFD]/gm;
  function escapeHarmfulChars(str){
    return (''+str).replace(/&/g, '&amp;')
      .replace(/</g, '&lt;')
      .replace(/>/g, '&gt;')
      .replace(/'/g, '&apos;')
      .replace(/"/g, '&quot;')
      .replace(not_safe_in_xml, '');
  }
  // QUERY URL TO GET PARAMS
  function urlquery(keyname){
    var match = false
    window.location.search
      .replace(/[?&]+([^=&]+)=([^&]*)/gi, function(str,k,v) {
        if( k == keyname) {
          param = v
          match = v
        }
      }
    );
    return match
  }
  // WINDOW RESIZE EVENT
  $(window).resize(function(){
    $(window).trigger('window:resize')
  })


  // FORM PROCESSING
  var processing_interval
  function processing (on, $form) {
    if ( on ) {
      $form.hide()
      $form.after('<span class="processing">Processing<br></span>')
      var processing_count = 1
      processing_interval = setInterval(function(){
        processing_count === 6 ? processing_count = 0 : processing_count++
        var dot = '.'
        $('.processing').html('Processing<br>' + dot.repeat(processing_count) + '&nbsp;')
      },100)
    } else {
      clearInterval(processing_interval)
      $('.processing').remove()
      $form.show()
    }
  }
</script>


