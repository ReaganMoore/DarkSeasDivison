<script>
  var $els = $('.simple-search-header .close, .simple-search-header form')
  function searchSetActive (open) {
    if (open) {
      $('.simple-search-header').addClass('active')
      $('.simple-search-header .open').hide()
      $els.css('display', 'inline')
    } else {
      $('.simple-search-header').removeClass('active')
      $('.simple-search-header .open').show()
      $els.hide()
    }
  }
  function clickOff (e, sel) {
    var el = document.querySelector(sel)
    var close = document.querySelector('.sx-close')
    if (!el) return
    var target = e.target
    console.log('target', target)
    do {
      // IF CLICKED ON EL OR CLOSE
      if (target == el) return
      if (target.tagName === 'BODY') return
      if (target == close) return searchSetActive()
      target = target.parentNode
    } while (target)
    searchSetActive()
  }
  function clickOffSearch (e) {
    clickOff(e, '.simple-search-header')
  }
  function openCloseSearch () {
    $('.simple-search-header .open').click(function(){
      searchSetActive(1)
    })
    $('.simple-search-header .close').click(function(){
      searchSetActive()
    })
  }
  $(function(){
    openCloseSearch()
    document.addEventListener('click', clickOffSearch, true)
  })
</script>

