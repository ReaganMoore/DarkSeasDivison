<script>
  function changeCurrency (c) {
    $('.multi-currency-select select option').prop('selected', false)
    $('.multi-currency-select select option').each(function(){
      if (c === $(this).val()) {
        $(this).prop('selected', true)
        $('.multi-currency-select form').submit()
      } 
    })
  }
  function currencySelect () {
    var $closed = $('.multi-currency-select .closed')
    var $open = $('.multi-currency-select .open')
    $closed.click(function(){
      $closed.hide()
      $open.show()
    })
  }
  function setMCTopMargin () {
    if (window.innerWidth > 1030) return
    $('.multi-currency-select').css('margin-top', $('header').outerHeight() + 'px')
  }
  $(function(){
    setMCTopMargin()
    currencySelect()
  })
</script>


