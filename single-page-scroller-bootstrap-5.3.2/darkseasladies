<script>
  var after_message = 'Rad! You\'re subscribed.'

  function form_success (type, res) {
    console.log('form_success', type, res)
    var options = {
      footer: {
        signup_location: 'Newsletter Subscribe - Footer'
      },
      overlay: {
        signup_location: 'Newsletter Subscribe - Overlay'
      },
      slide: {
        signup_location: 'Newsletter Subscribe - Slide'
      },
      waitlist: {
        signup_location: 'Waitlist - Footer',
      }
    }
    try {
      googleAnalytics('Subscribe', 'Newsletter',  options[type].signup_location)
      fbq('track', 'Lead')
    } catch (error) {}

    switch (type) {
      case 'footer':
        newsletterOverlayActivity(1, 1)
        
        if (res.data && res.data.code) {
          $('.newsletter-footer-body').html('<h2>Your discount code is:<p><b>'+res.data.code+'</b></p></h2>')
        } else {
          $('.newsletter-footer-body').html('<h2><b>' + nloConfig.success_message + '</b></h2>')
        }
        break
      case 'overlay':
        console.log('overlay success')
        newsletterOverlayActivity(1, 1)
        
        if ( res.data && res.data.code ) {
          $('.newsletter-overlay form').after('<div class="nlo_discount_after"><p>Here is your discount code for '+nloConfig.offer_text+':</p><div>' + res.data.code + '</div></div>')
        } else {
          $('.newsletter-overlay form').after('<strong>' + nloConfig.success_message + '</strong>')
        }
        $('.newsletter-overlay form').hide()
        break
      case 'slide':
        newsletterOverlayActivity(1, 1)
        
        $('.newsletter-slide form').after('<span>'+after_message+'</span>')
        $('.newsletter-slide form').remove()
        break
      case 'waitlist':
        $('.wbp_waitlist h3').html('<b>You are on the list!</b>')
        $('.wbp_waitlist form').hide()
    }
  }

  function draw_newsletter_footer(){
    $('.newsletter-footer').html(
      '<div class="newsletter-footer-body">\
        <h2>Sign up for '+nloConfig.offer_text+'</h2>\
        <form data-type="footer" class="wb_signup" data-list_id="'+nloConfig.list_id+'">\
          <div class="form-group">\
            <div class="btn-inline-submit">\
              <input class="form-control" type="email" value="" name="email" placeholder="Email..."/>\
              <button class="btn btn-primary"><i class="material-icons">chevron_right</i></button>\
            </div>\
          </div>\
        </form>\
        </div>\
      </div>\
      </div>'
    )
    $('.newsletter-footer').fadeIn(400)

  }

  function newsletterOverlayActivity (hide, success) {
    var cookieData = cookie('newsletter-activity')
    cookieData = cookieData ? JSON.parse(cookieData) : {
      view: {
        count_total: 0
      },
      hide: 0,
      success: 0
    }
    if (hide || success) {
      if (hide) cookieData.hide = 1
      if (success) cookieData.success = 1
      cookie('newsletter-activity', JSON.stringify(cookieData), {expires: 365})
      return
    }

    if (cookieData.hide) return

    var type = location.pathname === '/' ? 'home' : 'page'
    if(location.pathname.indexOf('/products') > -1) type = 'product'
    if(location.pathname.indexOf('/collections') > -1) type = 'collection'
    if(location.pathname.indexOf('/blogs') > -1) {
      type = location.pathname.split('/').length === 4 ? 'article' : 'blog'
    } 

    if (!cookieData.view[type]) cookieData.view[type] = 0
    cookieData.view[type]++
    cookieData.view.count_total++

    cookie('newsletter-activity', JSON.stringify(cookieData), {expires: 365})
  }

  function drawOverlayCheck (cb) {
    if (!nloConfig.on) return cb()

    var canShow = 0

    var cookieData = cookie('newsletter-activity')
    if (!cookieData) canShow = 1

    cookieData = JSON.parse(cookieData)

    if (cookieData.hide) return cb()

    var delay = nloConfig ? nloConfig.delay : 6
    var isProduct = location.href.indexOf('/products') > -1
    var isCollection = location.href.indexOf('/collections') > -1

    if (cookieData.view.count_total >= 3) canShow = 1
    if (isProduct) canShow = 1
    if (isCollection) canShow = 1
  
    if (!canShow) return cb()

    setTimeout(function () {
      cb(canShow)
    }, delay * 1000)
  }

  function drawNewsletterOverlay (){
    var existing = $('.newsletter-overlay') && $('.newsletter-overlay').length
    if (existing) return
    
    $('body').prepend(
      '<div class="newsletter-overlay">\
        <div class="newsletter-overlay-content" tabindex="0" aria-live="polite">\
          <div class="close"></div>\
          <div class="newsletter-overlay-title" tabindex="0">'+nloConfig.title+'</div>\
          <div class="newsletter-overlay-body">\
            <p tabindex="0">'+nloConfig.text+'</p>\
            <form data-type="overlay" class="wb_signup" data-list_id="'+nloConfig.list_id+'">\
              <div class="form-group">\
                <div class="btn-inline-submit">\
                  <input class="form-control" type="email" value="" name="email" placeholder="Email..."/>\
                  <button class="btn btn-primary"><i class="material-icons">chevron_right</i></button>\
                </div>\
              </div>\
            </form>\
          </div>\
        </div>\
      </div>'
    )
    $('.newsletter-overlay').fadeIn(400)

    focusEl('.newsletter-overlay-content')

    // RESIZE FOR NEWSLETTER CONTAINER
    var set_height = function(){
      $('.newsletter-overlay').css('height', window.innerHeight )
    }
    $(window).on('window:resize',function(e){
      set_height()
    })
    set_height()

    var closeNO = function (e) {
      if (e) {
        var isContent = e.target.closest('.newsletter-overlay-content')
        var isClose = e.target.classList.contains('close')
        if (isContent && !isClose) return
      }
      newsletterOverlayActivity(1)
      
      $('.newsletter-overlay').fadeOut(200)
      setTimeout(function () {
        $('.newsletter-overlay').remove()
      }, 300)
    }

    $('.newsletter-overlay-no-thanks').click(closeNO)

    // ON CLICK OUTSIDE
    // $(document).click(closeNO)
    $('.newsletter-overlay').click(closeNO)
    $(window).on('keyup', function(e){
      if (e.keyCode === 27) closeNO()
    })
  }

  function validateForm($form){
    // GET EMAIL
    var $newsletter_email = $form.find('input[name="email"]')
    var newsletter_email_val = $newsletter_email.val()

    if ( newsletter_email_val.indexOf('@') > -1 && newsletter_email_val.indexOf('.') > -1 ) {
      $newsletter_email.css('border-color','inherit')
      // EMAIL VALIDATES, SUBMIT
      return false
    } else {
      $newsletter_email.attr('style', 'border-color: #ff0000 !important')
      alert('Please enter a valid email address.')
      return true
    }
  }

  function selectForCopy (el) {
    var range = document.createRange()
    range.selectNodeContents(el)
    var sel = window.getSelection()
    sel.removeAllRanges()
    sel.addRange(range)
  }

  function discountReminder () {
    var discountCookie = cookie('wb_price_rule')
    if (!discountCookie) return
    if ($('.discount-prompt').length) return
    var code = discountCookie.split(',')[1]
    var promptHtml = '<div class="discount-prompt">\
        <div class="closed"><span class="discount-prompt-icon"></span><span class="discount-prompt-code">' + code + '</span></div>\
        <div class="open">\
          Your discount code is:<br><br><strong>' + code + '</strong>\
          <br><br><a href="#" class="used">I\'ve used this, thanks.</a>\
        </div>\
      </div>'
    $('header .R').append(promptHtml)
    $('.nav-mobile-options').append(promptHtml)
    $('.discount-prompt .closed').click(function() {
      var $parent = $(this).closest('.discount-prompt')
      $parent.addClass('active')
      var $open = $parent.find('.open')
      selectForCopy($open.find('strong')[0])
      $open[0].addEventListener('click', function (e) {
        var { target } = e
        var { tagName } = target
        if (tagName !== 'A') return $parent.removeClass('active')
        $('.discount-prompt').remove()
        cookie('wb_price_rule', null)
      }, true)
    })
  }

  function newsletterFormSubmit (e) {
    e.preventDefault()
    var type = $(this).attr('data-type')
    var email = $(this).find('input[name="email"]').val()
    var list_id = $(this).attr('data-list_id')

    if ( validateForm($(this)) ) return

    processing(true, $(this))

    var options = {
      vendor: 'mailchimp-subscribe',
      action: 'subscribe',
      email: email,
      signup_location: nloConfig.prefix + type,
      list_id: list_id
    }
    
      options.code = {
        text: nloConfig.code_prefix,
        append_unique_numbers: true
      }
      options.price_rule_id = nloConfig.price_rule_id
    
    wb_apihelper(options, function(err, res){
      processing(false, $(this))
      if ( err ) return alert(res.message)
      console.log(res)
      form_success(type, res)
      discountReminder()
    })
  }

  $(function(){
    newsletterOverlayActivity()

    drawOverlayCheck(function (canShowOverlay) {
      if (!canShowOverlay) return
      drawNewsletterOverlay()
      $('.wb_signup[data-type="overlay"]').submit(newsletterFormSubmit)
    })
    

    draw_newsletter_footer()
    $('.wb_signup[data-type="footer"]').submit(newsletterFormSubmit)

    discountReminder()

  })
</script>

